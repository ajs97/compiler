using System; 
// USING IDENTIFIER DELIM 
 
// 
namespace Loops 
// NAMESPACE IDENTIFIER 
{ 
// BLOCK_BEGIN 
 
// 
    class Program 
// CLASS IDENTIFIER 
    { 
// BLOCK_BEGIN 
        static void Main(string[] args) 
// STATIC VOID IDENTIFIER OPEN_PAREN STRING OPEN_BRACKET CLOSE_BRACKET IDENTIFIER CLOSE_PAREN 
        { 
// BLOCK_BEGIN 
            /* local variable definition */ 
// 
            int a = 10; 
// INT IDENTIFIER ASSIGN ICONST DELIM 
 
// 
            /* while loop execution */ 
// 
            while (a < 20) 
// WHILE OPEN_PAREN IDENTIFIER LT ICONST CLOSE_PAREN 
            { 
// BLOCK_BEGIN 
                Console.WriteLine("value of a: {0}", a); 
// IDENTIFIER DOT IDENTIFIER OPEN_PAREN SCONST COMMA IDENTIFIER CLOSE_PAREN DELIM 
                a++; 
// IDENTIFIER INCRE DELIM 
            } 
// BLOCK_END 
            Console.ReadLine(); 
// IDENTIFIER DOT IDENTIFIER OPEN_PAREN CLOSE_PAREN DELIM 
        } 
// BLOCK_END 
    } 
// BLOCK_END 
} 
// BLOCK_END 
