using System; 
// USING IDENTIFIER DELIM 
namespace RectangleApplication 
// NAMESPACE IDENTIFIER 
{ 
// BLOCK_BEGIN 
    class Rectangle 
// CLASS IDENTIFIER 
    { 
// BLOCK_BEGIN 
        // member variables 
// 
        double length; 
// DOUBLE IDENTIFIER DELIM 
        double width; 
// DOUBLE IDENTIFIER DELIM 
        public void Acceptdetails() 
// PUBLIC VOID IDENTIFIER OPEN_PAREN CLOSE_PAREN 
        { 
// BLOCK_BEGIN 
            length = 4.5; 
// IDENTIFIER ASSIGN DCONST DELIM 
            width = 3.5; 
// IDENTIFIER ASSIGN DCONST DELIM 
        } 
// BLOCK_END 
        public double GetArea() 
// PUBLIC DOUBLE IDENTIFIER OPEN_PAREN CLOSE_PAREN 
        { 
// BLOCK_BEGIN 
            return length * width; 
// RETURN IDENTIFIER TIMES IDENTIFIER DELIM 
        } 
// BLOCK_END 
        public void Display() 
// PUBLIC VOID IDENTIFIER OPEN_PAREN CLOSE_PAREN 
        { 
// BLOCK_BEGIN 
            Console.WriteLine("Length: {0}", length); 
// IDENTIFIER DOT IDENTIFIER OPEN_PAREN SCONST COMMA IDENTIFIER CLOSE_PAREN DELIM 
            Console.WriteLine("Width: {0}", width); 
// IDENTIFIER DOT IDENTIFIER OPEN_PAREN SCONST COMMA IDENTIFIER CLOSE_PAREN DELIM 
            Console.WriteLine("Area: {0}", GetArea()); 
// IDENTIFIER DOT IDENTIFIER OPEN_PAREN SCONST COMMA IDENTIFIER OPEN_PAREN CLOSE_PAREN CLOSE_PAREN DELIM 
        } 
// BLOCK_END 
    } 
// BLOCK_END 
 
// 
    class ExecuteRectangle 
// CLASS IDENTIFIER 
    { 
// BLOCK_BEGIN 
        static void Main(string[] args) 
// STATIC VOID IDENTIFIER OPEN_PAREN STRING OPEN_BRACKET CLOSE_BRACKET IDENTIFIER CLOSE_PAREN 
        { 
// BLOCK_BEGIN 
            Rectangle r = new Rectangle(); 
// IDENTIFIER IDENTIFIER ASSIGN NEW IDENTIFIER OPEN_PAREN CLOSE_PAREN DELIM 
            r.Acceptdetails(); 
// IDENTIFIER DOT IDENTIFIER OPEN_PAREN CLOSE_PAREN DELIM 
            r.Display(); 
// IDENTIFIER DOT IDENTIFIER OPEN_PAREN CLOSE_PAREN DELIM 
            Console.ReadLine(); 
// IDENTIFIER DOT IDENTIFIER OPEN_PAREN CLOSE_PAREN DELIM 
        } 
// BLOCK_END 
    } 
// BLOCK_END 
} 
// BLOCK_END 
