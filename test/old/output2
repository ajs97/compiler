using System; 
// USING IDENTIFIER DELIM 
namespace VariableDefinition 
// NAMESPACE IDENTIFIER 
{ 
// BLOCK_BEGIN 
    class Program 
// CLASS IDENTIFIER 
    { 
// BLOCK_BEGIN 
        static void Main(string[] args) 
// STATIC VOID IDENTIFIER OPEN_PAREN STRING OPEN_BRACKET CLOSE_BRACKET IDENTIFIER CLOSE_PAREN 
        { 
// BLOCK_BEGIN 
            short a; 
// SHORT IDENTIFIER DELIM 
            int b ; 
// INT IDENTIFIER DELIM 
            double c; 
// DOUBLE IDENTIFIER DELIM 
 
// 
            /* actual initialization */ 
// 
            a = 10; 
// IDENTIFIER ASSIGN ICONST DELIM 
            b = 20; 
// IDENTIFIER ASSIGN ICONST DELIM 
            c = a + b; 
// IDENTIFIER ASSIGN IDENTIFIER PLUS IDENTIFIER DELIM 
            Console.WriteLine("a = {0}, b = {1}, c = {2}", a, b, c); 
// IDENTIFIER DOT IDENTIFIER OPEN_PAREN SCONST COMMA IDENTIFIER COMMA IDENTIFIER COMMA IDENTIFIER CLOSE_PAREN DELIM 
            Console.ReadLine(); 
// IDENTIFIER DOT IDENTIFIER OPEN_PAREN CLOSE_PAREN DELIM 
        } 
// BLOCK_END 
    } 
// BLOCK_END 
} 
// BLOCK_END 
