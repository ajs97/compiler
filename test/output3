using System; 
// USING IDENTIFIER DELIM 
 
// 
namespace DecisionMaking 
// NAMESPACE IDENTIFIER 
{ 
// BLOCK_BEGIN 
 
// 
    class Program 
// CLASS IDENTIFIER 
    { 
// BLOCK_BEGIN 
        static void Main(string[] args) 
// STATIC VOID IDENTIFIER OPEN_PAREN STRING OPEN_BRACKET CLOSE_BRACKET IDENTIFIER CLOSE_PAREN 
        { 
// BLOCK_BEGIN 
 
// 
            int a = 100; 
// INT IDENTIFIER ASSIGN ICONST DELIM 
 
// 
            if (a < 20) 
// IF OPEN_PAREN IDENTIFIER LT ICONST CLOSE_PAREN 
            { 
// BLOCK_BEGIN 
                Console.WriteLine("a is less than 20"); 
// IDENTIFIER DOT IDENTIFIER OPEN_PAREN SCONST CLOSE_PAREN DELIM 
            } 
// BLOCK_END 
            else 
// ELSE 
            { 
// BLOCK_BEGIN 
                Console.WriteLine("a is not less than 20"); 
// IDENTIFIER DOT IDENTIFIER OPEN_PAREN SCONST CLOSE_PAREN DELIM 
            } 
// BLOCK_END 
            Console.WriteLine("value of a is : {0}", a); 
// IDENTIFIER DOT IDENTIFIER OPEN_PAREN SCONST COMMA IDENTIFIER CLOSE_PAREN DELIM 
            Console.ReadLine(); 
// IDENTIFIER DOT IDENTIFIER OPEN_PAREN CLOSE_PAREN DELIM 
        } 
// BLOCK_END 
    } 
// BLOCK_END 
} 
// BLOCK_END 
