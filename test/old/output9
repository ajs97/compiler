using System; 
// USING IDENTIFIER DELIM 
namespace EnumApplication 
// NAMESPACE IDENTIFIER 
{ 
// BLOCK_BEGIN 
   class EnumProgram 
// CLASS IDENTIFIER 
   { 
// BLOCK_BEGIN 
      enum Days { Sun, Mon, tue, Wed, thu, Fri, Sat }; 
// ENUM IDENTIFIER BLOCK_BEGIN IDENTIFIER COMMA IDENTIFIER COMMA IDENTIFIER COMMA IDENTIFIER COMMA IDENTIFIER COMMA IDENTIFIER COMMA IDENTIFIER BLOCK_END DELIM 
 
// 
      static void Main(string[] args) 
// STATIC VOID IDENTIFIER OPEN_PAREN STRING OPEN_BRACKET CLOSE_BRACKET IDENTIFIER CLOSE_PAREN 
      { 
// BLOCK_BEGIN 
         int WeekdayStart = (int)Days.Mon; 
// INT IDENTIFIER ASSIGN OPEN_PAREN INT CLOSE_PAREN IDENTIFIER DOT IDENTIFIER DELIM 
         int WeekdayEnd = (int)Days.Fri; 
// INT IDENTIFIER ASSIGN OPEN_PAREN INT CLOSE_PAREN IDENTIFIER DOT IDENTIFIER DELIM 
         Console.WriteLine("Monday: {0}", WeekdayStart); 
// IDENTIFIER DOT IDENTIFIER OPEN_PAREN SCONST COMMA IDENTIFIER CLOSE_PAREN DELIM 
         Console.WriteLine("Friday: {0}", WeekdayEnd); 
// IDENTIFIER DOT IDENTIFIER OPEN_PAREN SCONST COMMA IDENTIFIER CLOSE_PAREN DELIM 
         Console.ReadKey(); 
// IDENTIFIER DOT IDENTIFIER OPEN_PAREN CLOSE_PAREN DELIM 
      } 
// BLOCK_END 
   } 
// BLOCK_END 
} 
// BLOCK_END 
