using System; 
// USING IDENTIFIER DELIM 
 
// 
namespace Loops 
// NAMESPACE IDENTIFIER 
{ 
// BLOCK_BEGIN 
 
// 
    class Program 
// CLASS IDENTIFIER 
    { 
// BLOCK_BEGIN 
        static void Main(string[] args) 
// STATIC VOID IDENTIFIER OPEN_PAREN STRING OPEN_BRACKET CLOSE_BRACKET IDENTIFIER CLOSE_PAREN 
        { 
// BLOCK_BEGIN 
            /* for loop execution */ 
// 
            for (int a = 10; a < 20; a = a + 1) 
// FOR OPEN_PAREN INT IDENTIFIER ASSIGN ICONST DELIM IDENTIFIER LT ICONST DELIM IDENTIFIER ASSIGN IDENTIFIER PLUS ICONST CLOSE_PAREN 
            { 
// BLOCK_BEGIN 
                Console.WriteLine("value of a: {0}", a); 
// IDENTIFIER DOT IDENTIFIER OPEN_PAREN SCONST COMMA IDENTIFIER CLOSE_PAREN DELIM 
            } 
// BLOCK_END 
            Console.ReadLine(); 
// IDENTIFIER DOT IDENTIFIER OPEN_PAREN CLOSE_PAREN DELIM 
        } 
// BLOCK_END 
    } 
// BLOCK_END 
} 
// BLOCK_END 
